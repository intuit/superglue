{
  "name": "frontend",
  "version": "1.0.0",
  "description": "front end for superglue",
  "main": "index.jsx",
  "author": "Intuit",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn clean; webpack --config config/webpack-prod-config.js",
    "clean": "rm -rf build/*",
    "start:dev": "node config/webpack-dev-server.js",
    "start:prod": "yarn build; serve build",
    "lint": "eslint src -c .eslintrc.json --ext js,jsx",
    "check-staged": "lint-staged",
    "sass-lint": "sass-lint -vq src/**/*.scss",
    "test": "jest",
    "test:clear": "jest --clearCache; rm -rf coverage/",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage --colors"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "path": "^0.12.7",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.8.2",
    "react-redux": "^6.0.1",
    "redux-mock-store": "^1.5.3",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "serve": "^10.1.2",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-command": "^0.4.2",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@material/react-material-icon": "^0.12.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "connected-react-router": "^6.3.2",
    "dotenv": "^8.0.0",
    "elasticsearch": "^16.0.0",
    "history": "^4.9.0",
    "immutable": "^4.0.0-rc.12",
    "moxios": "^0.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-graph-vis": "^1.0.2",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-table": "^6.10.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ],
    "*.{js,jsx,json}": [
      "prettier --list-different --write",
      "git add"
    ],
    "src/**/*.scss": [
      "sass-lint -vq"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/setup/setupEnzyme.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/__tests__/setup/"
    ],
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.jsx",
      "!src/actions/AxiosInstance.js",
      "!src/reducers/index.js",
      "!src/store/**",
      "!src/middleware/**",
      "!src/enhancers/**",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "Actions(.*)$": "<rootDir>/src/actions/$1",
      "Components(.*)$": "<rootDir>/src/components/$1",
      "Constants(.*)$": "<rootDir>/src/constants/$1",
      "Containers(.*)$": "<rootDir>/src/containers/$1",
      "Store(.*)$": "<rootDir>/src/store/$1",
      "Styles(.*)$": "<rootDir>/src/styles/$1",
      "Reducers(.*)$": "<rootDir>/src/reducers/$1",
      "\\.(css|less|sass|scss)$": "<rootDir>/src/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "testMatch": [
      "**/__tests__/**/*.js?(x)"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
